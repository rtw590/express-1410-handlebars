 <div class="container">
    <h1 class="text-center mt-2 boldText">Downloads</h1>
    <h4 class="text-center">Art and Tutorial Files!</h4>

    {{# each products }}
        <div class="card-deck mt-3" >
            {{# each this }}
            <div class="card" >
                <a href="/downloads/{{ this.id }}"><img class="card-img-top" src="{{ this.imgPath }}" alt="Card image cap"></a>
                <div class="card-body">
                    <h5 class="card-title boldText">{{ this.title }}</h5>
                </div>
                <div class="card-footer">
                    <a href="/downloads/{{ this.id }}" class="btn btn-primary btn-block btn-danger card-bottom" style="margin-top: 5px;">View Product</a>
                </div>
            </div>
            {{/each}}
        </div> 
    {{/each}}

   

</div> {{!-- end container div --}}


